{
  "name": "@skandha/db",
  "private": "false",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.19",
  "description": "The DB module of Etherspot bundler client",
  "author": "Etherspot",
  "homepage": "https://github.com/etherspot/etherspot-bundler#readme",
